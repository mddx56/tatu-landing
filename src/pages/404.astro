---
import Container from "../components/Container.astro";
import Layout from "../layouts/Layout.astro";
---

<Layout title="TatuSoft - Precios">
  <main class="space-y-20">
    <Container>
      <div class="py-8 px-4 mx-auto max-w-screen-xl lg:py-16 lg:px-6">
        <div
          class="flex flex-col items-center py-3 md:w-6/12 md:pb-20 md:pt-10"
        >
          <img class="h-12" src="/hero/not-found.svg" alt="not found" />
          <h1 class="flex flex-grow">Pagina no encontrada.</h1>
        </div>
      </div>
    </Container>
  </main>
</Layout>
